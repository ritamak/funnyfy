<!-- Profile BUTTON -->
<!-- TITLE -->
<h1 id="jokeText">Joke library</h1>
<br>
<!-- GET RANDOM JOKE -->
<div id="randomJokes">
  <div id="programming">
  {{#each jokeProgramming}}
  {{this.setup}}
  {{this.punchline}}
  {{/each}}
  </div>
  <div id="knock">
  {{#each jokeKnock}}
  {{this.setup}}
  {{this.punchline}}
  {{/each}}
  </div>
  <div id="general">
  {{#each jokeGeneral}}
  {{this.setup}}
  {{this.punchline}}
  {{/each}}
  </div>
</div>
<!-- SEARCH FOR JOKES -->
<div class="search-container">
<form action="/GET">
  <input type="text" placeholder="search jokes" name="search">
  <button type="submit"><i class="fa fa-search"></i></button>
</form>
</div>
<!-- COLLECTION OF JOKES -->
<div id="cards">
  <div class="cardGeneral">
  <div class="card" style="width: 100%">
    <img src="" class="card-img-top" alt="lol image">
    <div class="card-body">
    <h5 class="card-title">General</h5>
    <p class="card-text">jokes for everyone</p>
    <a href="/main/general" title="profile"><i class="bi bi-caret-right-fill" style="font-size:48px"></i></a>
  </div>
  </div>
  </div>
  <div class="cardProgramming">
  <div class="card" style="width: 100%">
  <img src="/public/images/WHOS.jpg" class="card-img-top" alt="code image">
  <div class="card-body">
    <h5 class="card-title">Programming</h5>
    <p class="card-text">{ for the coders }</p>
    <a href="/main/programming" title="profile"><i class="bi bi-caret-right-fill" style="font-size:48px"></i></a>
  </div>
</div>
</div>
<div class="cardKnock">
<div class="card" style="width: 100%">
  <img src="/public/images/WHOS.jpeg" class="card-img-top" alt="yellow door">
  <div class="card-body">
    <h5 class="card-title">Knock-knock</h5>
    <p class="card-text">who is it?</p>
    <a href="/main/knock-knock" title="profile"><i class="bi bi-caret-right-fill" style="font-size:48px"></i></a>
  </div>
  </div>
</div>
</div>
