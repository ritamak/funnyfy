<h1 id="jokeText">Joke Library</h1>
<br>
<div class="container-img">
<img src="https://cdn.pixabay.com/photo/2020/02/07/12/54/emoji-4827091_960_720.png" id="main-img">
 <img src="https://cdn.pixabay.com/photo/2020/02/09/09/38/smiley-4832492_960_720.png" id="main-image">
 <img src="https://cdn.pixabay.com/photo/2020/02/07/12/38/emoji-4827049_1280.png" id="main-img">
 <img src="https://cdn.pixabay.com/photo/2020/02/09/09/39/smiley-4832494_960_720.png" id="main-image">
</div>
<br>
<!-- GET RANDOM JOKE -->
<div id="randomJokes">
  <div id="programming">
    {{#each jokeProgramming}}
    {{this.setup}}
    {{this.punchline}}
    {{/each}}
  </div>
  <div id="knock">
    {{#each jokeKnock}}
    {{this.setup}}
    {{this.punchline}}
    {{/each}}
  </div>
  <div id="general">
    {{#each jokeGeneral}}
    {{this.setup}}
    {{this.punchline}}
    {{/each}}
  </div>
</div>
<br>
<!-- COLLECTION OF JOKES -->
<div id="cards">
  <div class="cardGeneral">
    <div class="card" style="width: 100%">
      <img src="https://timelinecovers.pro/facebook-cover/download/flying-smileys-positive-facebook-cover.jpg" class="card-img-top" alt="lol image" id="img-smily">
      <div class="card-body">
        <h5 class="card-title" style="color: black;">General</h5>
        <a href="/main/general" title="profile"><i class="bi bi-caret-right-fill" style="font-size:48px"></i></a>
      </div>
    </div>
  </div>
  <div class="cardProgramming">
    <div class="card" style="width: 100%">
      <img src="https://step-tech.pl/wp-content/uploads/2020/10/1602318740_Hur-man-anvander-Emoji-for-att-namnge-filer-och-mappar.jpg" class="card-img-top" alt="code image">
      <div class="card-body">
        <h5 class="card-title"style="color:black">Programming</h5>
        <a href="/main/programming" title="profile"><i class="bi bi-caret-right-fill" style="font-size:48px"></i></a>
      </div>
    </div>
  </div>
  <div class="cardKnock">
    <div class="card" style="width: 100%">
      <img src="https://www.sleekcover.com/covers/smiley-pills-facebook-cover.jpg" class="card-img-top" alt="yellow door">
      <div class="card-body">
        <h5 class="card-title" style="color:black">Knock-knock</h5>
        <a href="/main/knock-knock" title="profile"><i class="bi bi-caret-right-fill" style="font-size:48px"></i></a>
      </div>
    </div>
  </div>
</div>
</div>
